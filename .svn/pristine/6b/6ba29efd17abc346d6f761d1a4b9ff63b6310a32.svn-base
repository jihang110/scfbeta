				{
					"name": "推荐",
					"icon": "fa fa-plus",
					"action": "eb.app.buyerCntForm.doRecommand(@{DATAINDEX},'@{isRecom}')"
			  	}
	        ]
	      }
        ]
      },
      {
        "type": "datagrid",
        "name": "upLoadList",
        "id": "upLoadList",
