    "pageType": "page-workflow",
    "baseUrl": "/pub/buyercntrct/bpm",
    
  "fields": [
      {
        "type": "hidden",
        "name": "recUid"
      },
       {
        "type": "text",
        "label": "协议编号",
        "name": "cntNo",
