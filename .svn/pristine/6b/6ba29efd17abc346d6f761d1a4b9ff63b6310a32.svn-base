      {
        "type": "text",
        "label": "协调人名称",
        "name": "coordinatorNm",
        "id": "coordinatorNm",
        "required": true
      },
      {
        "type": "text",
        "label": "电子邮箱",
        "name": "coordinatorEmail",
        "id": "coordinatorEmail",
