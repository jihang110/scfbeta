package com.ut.scf.reqbean.pubmanage;

import java.math.BigDecimal;
import java.util.List;

public class ProductDistributionReqBean {

	/**
	 * 买方ID
	 */
	private String corpId;
	/**
	 * 买方名称
	 */
	private String corpName;

	/**
	 * 组织机构代码
	 */
	private String orgnNo;
	/**
	 * 授信额度
	 */
	private BigDecimal lmtAmt;

	/**
	 * 可分配授信额度
	 */
	private BigDecimal releLmtAmt;

	private String createUser;

	private List<ProductDistributionBean> prdDistList;

	public BigDecimal getReleLmtAmt() {
		return releLmtAmt;
	}

	public void setReleLmtAmt(BigDecimal releLmtAmt) {
		this.releLmtAmt = releLmtAmt;
	}

	public String getCreateUser() {
		return createUser;
	}

	public void setCreateUser(String createUser) {
		this.createUser = createUser;
	}

	public String getCorpId() {
		return corpId;
	}

	public void setCorpId(String corpId) {
		this.corpId = corpId;
	}

	public String getCorpName() {
		return corpName;
	}

	public void setCorpName(String corpName) {
		this.corpName = corpName;
	}

	public String getOrgnNo() {
		return orgnNo;
	}

	public void setOrgnNo(String orgnNo) {
		this.orgnNo = orgnNo;
	}

	public BigDecimal getLmtAmt() {
		return lmtAmt;
	}

	public void setLmtAmt(BigDecimal lmtAmt) {
		this.lmtAmt = lmtAmt;
	}

	public List<ProductDistributionBean> getPrdDistList() {
		return prdDistList;
	}

	public void setPrdDistList(List<ProductDistributionBean> prdDistList) {
		this.prdDistList = prdDistList;
	}

}
