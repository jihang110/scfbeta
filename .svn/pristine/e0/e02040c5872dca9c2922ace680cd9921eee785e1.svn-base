{
  "result": 0,
  "resultData": {
    "pageType": "page-form",
    "submitUrl": "/user/add",
    "formId": "userForm",
    "idField": "userId",
    "title": "新增用户",
    "fields": [
      {
        "type": "hidden",
        "name": "userId"
      },
      {
        "type": "hidden",
        "name": "corpId"
      },
      {
        "type": "text",
        "label": "用户名称",
        "name": "username",
        "required": true
      },
      {
        "type": "select",
        "label": "所属企业",
        "name":"corpId",
        "id":"user-corpId"
      },
      {
        "type": "password",
        "label": "密码",
        "name": "password",
        "required": true
      },
      {
        "id": "ipt-confirmPwd",
        "type": "password",
        "label": "确认密码",
        "required": true
      },

      {
        "type": "text",
        "label": "手机号码",
        "name": "mobilephone",
        "required": true,
        "reg": "mobilephone"
      },
      {
        "type": "text",
        "label": "真实姓名",
        "name": "realname",
        "required": true,
        "reg": "string"
      },
      {
        "type": "text",
        "label": "邮箱",
        "name": "email",
        "reg": "email"
      },
      {
        "type": "text",
        "label": "固定电话",
        "name": "fixedPhone",
        "required": true,
        "reg": "fixedPhone"
      },

      {
        "type": "textarea",
        "label": "用户描述",
        "name": "note"
      },
      {
        "id": "dg-user-roleList",
        "type": "datagrid",
        "name": "deptRoles",
        "btnGroup":
        [
          {
            "type": "grid-form-win",
            "title": "新增",
            "icon": "fa fa-plus",
            "route": "userRoleForm-add"
          }
        ],

        "idField": "deptId",
        "columns":
        [
          {
            "title": "机构名称",
            "dictName": "DEPT_NAME",
            "field": "mainDeptId"
          },
          {
            "title": "部门角色",
            "dictName": "ROLE_NAME",
            "field": "mainRoleId"
          },

          {
            "title": "操作",
            "actions":
            [
              {
                "name": "编辑",
                "icon": "fa fa-pencil",
                "action": "eb.app.openItemForm(@{DATAINDEX}, 'userRoleForm-add', 'deptRoles')"
              },
              {
                "name": "删除",
                "icon": "fa fa-trash",
                "action": "$('#dg-user-roleList').EBDataGrid('delData', {index: @{DATAINDEX}})"
              }
            ]
          }
        ]
      }
    ],
    "btnGroup": [
      {
        "id": "sys-user-btn",
        "title": "保存",
        "icon": "fa fa-paper-plane",
        "type": "custom",
        "submitUrl": "/user/add"
      }
    ],
    "script": "business/sys/js/userManager-add.js"
  },
  "resultMsg": null
}